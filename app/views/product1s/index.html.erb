<h1>Listing product1s</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Categoryies</th>
    <th>Price</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product1s.each do |product1| %>
  <tr>
    <td><%= product1.name %></td>
    <td><%= product1.categoryies %></td>
    <td><%= product1.price %></td>
    <td><%= product1.quantity %></td>
    <td><%= link_to 'Show', product1 %></td>
    <td><%= link_to 'Edit', edit_product1_path(product1) %></td>
    <td><%= link_to 'Destroy', product1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product1', new_product1_path %>
